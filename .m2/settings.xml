<?xml version="1.0" encoding="UTF-8"?>
<settings>
    <servers>
        <server>
            <username>${env.CI_ARTIFACTORY_USER}</username>
            <password>${env.CI_ARTIFACTORY_PASSWORD}</password>
            <id>ga-central</id>
        </server>
        <server>
            <username>${env.CI_ARTIFACTORY_USER}</username>
            <password>${env.CI_ARTIFACTORY_PASSWORD}</password>
            <id>ga-snapshots</id>
        </server>
    </servers>
    <profiles>
        <profile>
            <repositories>
                <repository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>ga-central</id>
                    <name>libs-release</name>
                    <url>https://artifactory.graphaware.com/artifactory/libs-release</url>
                </repository>
                <repository>
                    <snapshots />
                    <id>ga-snapshots</id>
                    <name>libs-snapshot</name>
                    <url>https://artifactory.graphaware.com/artifactory/libs-snapshot</url>
                </repository>
                <repository>
                    <id>central</id>
                    <name>Maven Repository Switchboard</name>
                    <layout>default</layout>
                    <url>http://repo1.maven.org/maven2</url>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>ga-central</id>
                    <name>libs-release</name>
                    <url>https://artifactory.graphaware.com/artifactory/libs-release</url>
                </pluginRepository>
                <pluginRepository>
                    <snapshots />
                    <id>ga-snapshots</id>
                    <name>libs-snapshot</name>
                    <url>https://artifactory.graphaware.com/artifactory/libs-snapshot</url>
                </pluginRepository>
            </pluginRepositories>
            <id>artifactory</id>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>artifactory</activeProfile>
    </activeProfiles>
</settings>
